{
  "name": "gamebox-web-server",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "install:all": "pnpm -w install",
    "dev:backend": "pnpm -w --filter ./packages/backend dev",
    "dev:frontend": "pnpm -w --filter ./packages/frontend dev",
    "dev": "concurrently \"pnpm dev:backend\" \"pnpm dev:frontend\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
